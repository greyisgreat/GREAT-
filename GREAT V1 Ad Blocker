javascript:(()=>{if(window.GREAT_ADBLOCKER_LOADED)return;window.GREAT_ADBLOCKER_LOADED=1;const s=document.createElement("style");s.textContent=`#GREAT_UI{position:fixed;top:20px;right:20px;z-index:999999999;background:#1e1e1e;color:#fff;border:2px solid #444;padding:14px;border-radius:12px;font-family:sans-serif;box-shadow:0 0 12px rgba(0,0,0,.6);width:200px}#GREAT_UI button{width:100%;margin-top:10px;padding:8px;background:#333;border:1px solid #666;border-radius:8px;color:#fff;cursor:pointer;font-size:14px;font-weight:600}#GREAT_UI button:hover{background:#444}#GREAT_UI .title{text-align:center;font-size:16px;font-weight:700;margin-bottom:8px}#GREAT_GRID{position:fixed;top:0;left:0;width:100vw;height:100vh;display:grid;grid-template-columns:repeat(12,1fr);grid-template-rows:repeat(8,1fr);z-index:999999900;}#GREAT_GRID div{border:1px solid rgba(255,255,255,.05);}#GREAT_GRID div:hover{background:rgba(255,0,0,.2);cursor:pointer;}`;document.body.appendChild(s);const ui=document.createElement("div");ui.id="GREAT_UI";ui.innerHTML=`<span class="title">GREAT Ad Blocker</span><button id="g_sel">Grid Select</button><button id="g_all">Block All Ads</button><button id="g_vid">Block Video Ads</button>`;document.body.appendChild(ui);const remove=e=>e&&e.remove();const adSelectors=['[class*="ad"]','[id*="ad"]','div[id^="google_ads"]','iframe[src*="ad"]','[aria-label="advertisement"]'];const blockAll=()=>adSelectors.forEach(sel=>document.querySelectorAll(sel).forEach(remove));const blockVideos=()=>document.querySelectorAll("video,iframe").forEach(v=>{if((v.src||"").toLowerCase().includes("ad")||(v.className||"").toLowerCase().includes("ad"))remove(v)});function gridSelect(){const grid=document.createElement("div");grid.id="GREAT_GRID";for(let i=0;i<96;i++){const c=document.createElement("div");c.onclick=()=>{const r=c.getBoundingClientRect();document.querySelectorAll("*").forEach(el=>{const b=el.getBoundingClientRect();if(b.top>=r.top&&b.bottom<=r.bottom&&b.left>=r.left&&b.right<=r.right&&el!==grid&&el!==ui)remove(el)});grid.remove();};grid.appendChild(c);}document.body.appendChild(grid);}document.getElementById("g_sel").onclick=gridSelect;document.getElementById("g_all").onclick=blockAll;document.getElementById("g_vid").onclick=blockVideos})();
